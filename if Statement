alien_0 = {"x_position": 0, "speed": "medium"}
if  alien_0["speed"] == 'medium':
    x_increment = 2
elif alien_0["speed"] == 'slow':
    x_increment = 1
elif alien_0["speed"] == 'fast':
    x_increment = 3
else:
    x_increment = 0
alien_0["x_position"] += x_increment
print(f"The new position of alien0 is {x_increment}.")
